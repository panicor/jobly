{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "salary": {
        "type": "integer",
        "minimum": 0
      },
      "equity": {
        "type": "string",
        "pattern": "0|(0?\\.[0-9]+)"
      }
    },
    "required": [
      "title"
    ]
  }
  